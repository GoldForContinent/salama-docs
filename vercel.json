{
  "buildCommand": "echo 'Static site'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/reportlost",
      "destination": "/reportlost.html"
    },
    {
      "source": "/reportfound",
      "destination": "/reportfound.html"
    },
    {
      "source": "/digital-locker",
      "destination": "/digital-locker.html"
    },
    {
      "source": "/admin-dashboard",
      "destination": "/admin-dashboard.html"
    },
    {
      "source": "/admin-login",
      "destination": "/admin-login.html"
    },
    {
      "source": "/login",
      "destination": "/loginpage.html"
    },
    {
      "source": "/signup",
      "destination": "/signup.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/faq",
      "destination": "/faq.html"
    },
    {
      "source": "/help",
      "destination": "/help.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/settings",
      "destination": "/settings.html"
    },
    {
      "source": "/debug",
      "destination": "/debug.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
